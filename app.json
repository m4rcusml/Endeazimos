{
  "expo": {
    "name": "Endeazimos",
    "slug": "Endeazimos",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#125266"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.m4rcusml.Endeazimos",
      "googleServicesFile": "./google-services.json",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDkOKcssFsmIeMsHFN3UdPx3FDRXmOrqLg"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#125266"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "@react-native-google-signin/google-signin",
      [
        "expo-image-picker",
        {
        "photosPermission": "O aplicativo acessa suas fotos para deixar você compartilhá-las com outras pessoas."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "7861fd28-d2e3-4cc9-a17f-592ed53c02fd"
      }
    }
  }
}